{
  "name": "@mobius-network/wallet",
  "version": "0.0.1",
  "description": "Mobius Network mobile wallet",
  "license": "MIT",
  "scripts": {
    "postinstall": "yarn run install:gems && yarn run install:hacks && yarn run install:pods",
    "install:hacks": "yarn run patch-package",
    "install:gems": "bundle check || bundle install",
    "install:pods": "diff ios/Podfile.lock ios/Pods/Manifest.lock || bundle exec pod install --project-directory=ios",
    "secrets": "bundle exec dotgpg cat .env.gpg > .env && bundle exec dotgpg cat .env.production.gpg > .env.production",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:fresh": "npm start -- --reset-cache",
    "link": "react-native link",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "prettier": "npx prettier --write src/**/*.js *.js",
    "lint": "npx eslint --fix --ext js .",
    "format": "npm run prettier && npm run lint",
    "create:component": "plop component",
    "create:state": "plop state",
    "clean": "yarn run clean:metro && rm -rf ./ios/build ./android/build",
    "clean:metro": "watchman watch-del-all && rm -rf $TMPDIR/{react,metro,haste}*"
  },
  "dependencies": {
    "@tradle/react-native-http": "^2.0.1",
    "assert": "^1.4.1",
    "asyncstorage-down": "^4.0.1",
    "axios": "^0.18.0",
    "base64-js": "^1.3.0",
    "coinmarketcap-api": "^2.3.2",
    "crypto-browserify": "^3.12.0",
    "d3-ease": "^1.0.3",
    "event-filter-redux-middleware": "^0.0.2",
    "events": "^3.0.0",
    "https-browserify": "^1.0.0",
    "i18next": "^11.5.0",
    "immutability-helper": "^2.7.0",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.10",
    "path-browserify": "^1.0.0",
    "prop-types": "^15.6.2",
    "query-string": "^6.1.0",
    "react": "16.4.1",
    "react-dom": "^16.5.0",
    "react-i18next": "^7.7.0",
    "react-move": "^2.7.0",
    "react-native": "0.57.0-rc.4",
    "react-native-bip39": "^2.3.0",
    "react-native-camera": "^1.2.0",
    "react-native-check-box": "^2.1.3",
    "react-native-custom-actionsheet": "^0.0.4",
    "react-native-device-info": "^0.22.4",
    "react-native-easy-grid": "0.2.0",
    "react-native-event-source": "^1.0.1",
    "react-native-fabric": "^0.5.1",
    "react-native-i18n": "^2.0.15",
    "react-native-keychain": "^3.0.0",
    "react-native-level-fs": "^3.0.1",
    "react-native-linear-gradient": "2.4.0",
    "react-native-modal": "^6.5.0",
    "react-native-permissions": "^1.1.1",
    "react-native-qrcode-svg": "^5.1.0",
    "react-native-randombytes": "^3.5.0",
    "react-native-splash-screen": "^3.1.1",
    "react-native-svg": "^6.5.2",
    "react-native-vector-icons": "^5.0.0",
    "react-navigation": "^2.11.2",
    "react-redux": "^5.0.7",
    "readable-stream": "^3.0.2",
    "redux": "^4.0.0",
    "redux-boost": "^0.0.19",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "redux-yo": "^0.0.26",
    "reselect": "^3.0.1",
    "scrypt-async": "^2.0.0",
    "stellar-hd-wallet": "^0.0.7",
    "stellar-sdk": "^0.10.2",
    "styled-components": "^3.4.1",
    "tweetnacl": "^1.0.0",
    "url": "^0.11.0",
    "vm-browserify": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-react-native": "^3.2.1",
    "flow-bin": "^0.78.0",
    "immutable": "^3.7.4",
    "jest": "22.4.3",
    "lint-staged": "^7.2.0",
    "metro-react-native-babel-preset": "0.45.1",
    "patch-package": "^5.1.1",
    "postinstall-postinstall": "^1.0.0",
    "prettier": "^1.12.1",
    "react-native-create-bridge": "^2.0.1",
    "react-native-dotenv": "^0.2.0",
    "react-test-renderer": "16.4.2",
    "rimraf": "2.6.2",
    "rn-nodeify": "tradle/rn-nodeify",
    "wsrun": "^2.2.0"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  },
  "engines": {
    "node": "8.11.3"
  },
  "devEngines": {
    "node": ">= 8.11.3",
    "npm": ">= 6.2",
    "yarn": ">= 1.9"
  }
}
